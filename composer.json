{
    "name": "mediadreams/md_newsfrontend",
    "type": "typo3-cms-extension",
    "description": "This extension enables frontend users to created news records in the frontend.",
    "keywords": [
        "TYPO3",
        "extension",
        "news",
        "frontend"
    ],
    "authors": [
        {
            "name": "Christoph Daecke",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^8.7.0|| ^9.5.0"
    },
    "suggest": {
        "georgringer/news": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Mediadreams\\MdNewsfrontend\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mediadreams\\MdNewsfrontend\\Tests\\": "Tests"
        }
    },
    "replace": {
        "md_newsfrontend": "self.version",
        "typo3-ter/md-newsfrontend": "self.version"
    }
}